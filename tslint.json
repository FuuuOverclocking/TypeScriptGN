{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ],
        "ban-types": false,
        "curly": [
            true,
            "ignore-same-line"
        ],
        "indent": [
            true,
            "spaces",
            4
        ],
        "interface-name": false,
        "max-classes-per-file": false,
        "max-line-length": false,
        "member-access": false,
        "member-ordering": false,
        "no-angle-bracket-type-assertion": false,
        "no-bitwise": false,
        "no-empty": false,
        "no-conditional-assignment": false,
        "no-consecutive-blank-lines": [
            true,
            2
        ],
        "no-console": false,
        "no-empty-interface": false,
        "no-namespace": false,
        "no-unused-expression": false,
        "object-literal-sort-keys": false,
        "prefer-for-of": false,
        "quotemark": [
            true,
            "single"
        ],
        "no-shadowed-variable": false,
        "trailing-comma": [
            true,
            {
                "multiline": {
                    "objects": "always",
                    "arrays": "always",
                    "functions": "never",
                    "typeLiterals": "ignore"
                },
                "esSpecCompliant": true
            }
        ],
        "unified-signatures": false,
        "variable-name": false,
        "whitespace": [
            false,
            "check-typecast"
        ],
    },
    "rulesDirectory": []
}